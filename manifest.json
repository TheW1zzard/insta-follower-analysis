{
  "name": "insta-follower-checker",
  "description": "Get frontend stats of your Instagram page",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "storage",
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["action.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "run"
  },
  "manifest_version": 2
}
